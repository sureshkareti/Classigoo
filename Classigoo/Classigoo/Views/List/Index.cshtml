<script>
    $(document).ready(function () {

        $(".autoplay").slick({

            variableWidth: true,

            slidesToShow: 5,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 2000,
            arrows: true,
            dots: false,
            pauseOnHover: false,

            responsive: [

                {
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 3
                    }
                }, {
                    breakpoint: 480,
                    settings: {
                        variableWidth: false,
                        slidesToShow: 2,

                    }
                }]
        });

        $(".autoplay-RE").slick({

            variableWidth: true,

            slidesToShow: 5,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 2000,
            arrows: true,
            dots: false,
            pauseOnHover: false,
            responsive: [

                {
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 3
                    }
                }, {
                    breakpoint: 480,
                    settings: {
                        variableWidth: false,
                        slidesToShow: 2,

                    }
                }]
        });
        $("#btnSearch").css({"background-color": "white", "border": "1px solid #337ab7", "color": "#337ab7","padding": "1px 6px"});
        $('#page-selection').bootpag({
            total: @Model.PageCount,
            page:@Model.CurrentPageIndex,
            leaps: true,
            firstLastUse: true,
            first: 'First',
            last: 'Last'
        }).on('page', function(event, num){
            $(".loader-wrap").show();
            filterAdds("",num,false);
        })
        FillFiltersFrmHomePage();
    });



    function FillFiltersFrmHomePage() {
        $('#listing_catagory').val('@ViewBag.FilterValues.Category').attr("selected", "selected");
        $("#listing_rent_listGeneral").val('@ViewBag.FilterValues.Type');
        $('#listing_location_list').val('@ViewBag.FilterValues.Location');
        $('#listing_keword').val('@ViewBag.FilterValues.SearchKeyword');
        ShowCategoryFilter('@ViewBag.FilterValues.Category');

        var categorySelected = "@ViewBag.FilterValues.Category";
        var subCatSelected='@ViewBag.FilterValues.SubCategory';

      
        subCatSelected = subCatSelected.replace("&amp;","&");

        //subCatSelected.replace(+%26+

        if(subCatSelected!="")
        {
            SetSubCatValue(subCatSelected);
        }
        //$(".scrollHide").css("display","none");
        if(categorySelected == "Passenger Vehicles"){
            $("#scrollPV").css("display","block");
        }
        else if(categorySelected == "Transportation Vehicles"){
            $("#scrollTV").css("display","block");
        }
        else if(categorySelected == "Construction Vehicles"){
            $("#scrollCV").css("display","block");
        }
        else if(categorySelected == "Agricultural Vehicles"){
            $("#scrollAV").css("display","block");
        }
        else if(categorySelected == "Real Estate"){

            $("#scrollRE").css("display","block");
        }
        else{
            $("#scrollAll").css("display","block");
        }



    }
</script>
