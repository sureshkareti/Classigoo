<script type="text/javascript">

    $(document).ready(function() {


        if("@ViewBag.Name" != ""){

            $("#Name").val("@ViewBag.Name");
            $("#PhoneNumber").val("@ViewBag.Number");

            $("#Name").attr("readonly", "true");
            $("#PhoneNumber").attr("readonly", "true");
        }

        if("@ViewBag.Message" == "error"){
            alert("error occured while post please try again..")
        }

        if("@ViewBag.Message" == "nologin"){
            alert("user not logged into ...please login and try again")
        }

        if("@ViewBag.Message" == "sucess"){

            var addId = "@ViewBag.addId";
            $("#spnAddId").text(addId);
            $("#addSuccessAlert").css("display","block");

        }
        if("@ViewBag.Message" == "updated"){

            var addId = "@ViewBag.addId";
            $("#spnAddIdUpdated").text(addId);
            $("#addUpdatedAlert").css("display","block");

        }

        

    });



</script>

